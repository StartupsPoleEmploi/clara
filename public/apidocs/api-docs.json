          "application/json",
        ],
        "produces": [
          "application/json",
        ],
        "parameters": [
        {
          "in": "header",
          "name": "Authorization",
          "type": "string",
          "description": "Bearer [your JWT token here]",
          "required": true
        },
        {
          "in": "path",
          "name": "aidSlug",
          "type": "string",
          "description": "Slug of aid",
          "required": true
