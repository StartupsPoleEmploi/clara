<%= form_with url: admin_post_cache_path, local: false, id: 'admin_post_cache_form' do |form| %>
  <%= form.submit 'Supprimer le cache de l\'accueil, rules, aides actives, contract_types et filtres' %>
<% end %>
<p>&nbsp;</p>
<script> 
  function defer2(method) { if (window.jQuery) { admin_post_cache_callback(); } else { setTimeout(function() { defer2(method) }, 50); } }
  function admin_post_cache_callback() {
    $("#admin_post_cache_form").on("ajax:success", 
      function(event, data) { 
        console.log("admin_post_cache success, detail below : "); 
        console.log(event); 
        console.log(data); 
      });     
  }
  defer2();
</script>
