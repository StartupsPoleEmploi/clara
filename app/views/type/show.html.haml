- @loaded ||= {}
- v = view_object(:type_show, {contract_type: local_assigns[:contract_type], aids: local_assigns[:aids]})
- title_data v.title_of_tab

= render 'shared/breadcrumb', {where: 'detail'}

%main#main.c-type-show{role: "main"}

  %h2.c-detail-title{"class" => v.clazz}
    .c-detail-title-inside.u-useful-width.u-margin-constraint
      =v.title

  .c-detail-show2.u-useful-width.u-margin-constraint
    .o-layout
      .o-layout__item{:class => "u-1/1 u-3/4@desktop"}
        - if v.has_line
          = render 'shared/result_line', {line: v.line, contract_type: v.contract_type}
      .o-layout__item{:class => "u-1/1 u-1/4@desktop"}
        = render 'shared/detail_void'

