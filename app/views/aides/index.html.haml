
- has_user = !params[:for_id].blank?
- if has_user
  - title_data "Vos résultats"
- else 
  - title_data "Découvrez toutes les aides et mesures de retour à l'emploi"
  
- loaded = @loaded || {}

= render 'shared/breadcrumb', {where: 'finals'}

%main#main.c-results-index{role: 'main'}
  - if has_user
    = local_assigns
  - else
    = render 'shared/result_all', {lines: FilterRawAidsService.new(@loaded['aids']).go}
      
