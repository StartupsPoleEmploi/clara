.c-result-all.u-useful-width.u-margin-constraint
  %h1 Toutes nos aides
  .o-layout
    .o-layout__item{:class => "u-1/1 u-3/4@desktop"}
      - local_assigns["aids"].group_by{|e| e["contract_type_id"]}.each do |aids|
        = aids[0]["contract_type_id"]
        = render 'shared/result_line', {aids: aids, contract: []}
    
    .o-layout__item{:class => "u-1/1 u-1/4@desktop"}
      = render 'shared/detail_void'
